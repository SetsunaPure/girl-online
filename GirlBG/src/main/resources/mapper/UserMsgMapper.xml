<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.girl.core.mapper.UserMsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.girl.core.entity.UserMsg">
        <id column="id" property="id" />
        <result column="type" property="type" />
        <result column="sub_type" property="subType" />
        <result column="msg" property="msg" />
        <result column="create_time" property="createTime" />
        <result column="uid" property="uid" />
        <result column="param" property="param" />
        <result column="bind_id" property="bindId" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, type, sub_type AS subType, msg, create_time AS createTime, uid, param, bind_id AS bindId, status
    </sql>

</mapper>
