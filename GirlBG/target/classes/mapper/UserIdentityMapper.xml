<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.girl.core.mapper.UserIdentityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.girl.core.entity.UserIdentity">
        <id column="uid" property="uid" />
        <result column="jgpass" property="jgpass" />
        <result column="wy_accid" property="wyAccid" />
        <result column="coin" property="coin" />
        <result column="zhifubao" property="zhifubao" />
        <result column="vip_expiry_date" property="vipExpiryDate" />
        <result column="vip" property="vip" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uid, jgpass, wy_accid AS wyAccid, coin, zhifubao, vip_expiry_date AS vipExpiryDate, vip
    </sql>

</mapper>
