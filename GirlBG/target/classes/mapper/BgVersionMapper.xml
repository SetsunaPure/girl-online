<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.girl.mapper.BgVersionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.girl.core.entity.BgVersion">
        <id column="id" property="id" />
        <result column="url" property="url" />
        <result column="info" property="info" />
        <result column="version_name" property="versionName" />
        <result column="version_code" property="versionCode" />
        <result column="update_type" property="updateType" />
        <result column="create_time" property="createTime" />
        <result column="phone_type" property="phoneType" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, url, info, version_name AS versionName, version_code AS versionCode, update_type AS updateType, create_time AS createTime, phone_type AS phoneType
    </sql>

</mapper>
